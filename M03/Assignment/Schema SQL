CREATE TABLE players(
  id INTEGER PRIMARY KEY,
  first_name TEXT,
  last_name TEXT,
  age INTEGER,
  team_id INTEGER,
  FOREIGN KEY(team_id) REFERENCES teams(team_id)
  );
  
  INSERT INTO players (id, first_name, last_name, age,team_id)
